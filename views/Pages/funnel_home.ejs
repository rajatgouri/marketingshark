  function deleteStep(e) {
    e.preventDefault()
    confirmation(
      "Are you sure?",
      "You won't be able to revert this!",
      "Yes, delete it!"
    )
    .then((result) => {
      
      if (result.isConfirmed) {

        const stepId = e.target.parentElement.getAttribute('id');
        e.target.innerText = 'Wait ...'
        $.ajax({
                type: "GET",
                url: `/delete-step/<%- funnel._id %>/${stepId}`,
                success: function (data) {
                  console.log(data)
                  window.location =
              `/funnel_home/<%- funnel._id %>?stepid=${data.stepId}`;
                  
                },
                error: function (data) {
                  console.log(data)
                  e.target.innerText = 'DELETE PAGE'

                  Swal.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: "Can't delete step if only One step Exist",
                  })
                }
            });


     
      }
      
    })
    .catch(err => {
      window.location.reload();
    })

  }

  function myFunction() {
    document.getElementById('myFilter').style.opacity = '27%';
  }
</script>
<script>
  function myFunctionNew() {
    document.getElementById('myFilter').style.opacity = '1';
  }
</script>
