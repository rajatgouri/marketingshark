<%- include('../Partials/header.ejs') %>
<div
  class="container"
  style="padding-right: 0px !important; padding-left: 0 !important;"
>
  <div class="row">
    <div class="col-md-12 filter_content_1">
      <span> Lead Capture for Real Estate…</span>
      <div class="bottom_left">
        <a href="/templates"
          ><u><i class="fa fa-angle-left"></i>Back to funnels</u></a
        >
      </div>
      <div class="funnel_home">
        <div class="col-lg-3 my_sidebar" id="myFilter">
          <div class="sidebar main_div">
            <p>Funnel Steps</p>
            <div class="sidebar_buttons">
              <% funnel.steps.map(step => { %>
              <div class="btn1">
                <a
                  href="/funnel_home/<%= funnel.id %>?stepid=<%= stepid=step.id %>"
                  class="list-group-item steps"
                  id="<%= step.id %>"
                  ><img
                    src="/images/Page-in-opt.svg"
                    alt="group_40"
                    width="18%" />
                  <span style="color: #fff; padding-left: 26px;">
                    <%= step.stepName.split('_').length > 2 ? step.stepName.split('_').slice(0,-1).join('_') :  step.stepName.split('_').join('_') %></span
                  >
                  <img src="/images/white arrow.svg" width="3%" alt="Polygon_4"
                /></a>
              </div>
              <% }) %>
              <!-- <div class="btn2">
                <a href="#" class="list-group-item border"
