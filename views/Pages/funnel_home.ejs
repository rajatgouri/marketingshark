<script>
  jQuery(() => {

    let backButton = $('.integration-back-button');
    let integrationTabs = $('.integrations-tab');
    let funnelDetails = $('.search_form_settings');
    let addPayment = $('#add-payment');
    let removePayment = $('#remove-payment');
    let selected = $('.payments')

    backButton.hide();
    removePayment.hide();
    integrationTabs.hide();

    addPayment.on('click', function (e) {
      let funnel = selected.attr('funnel');
      let payment = selected.val();

      if(!payment) {
        return alert('please select a payment');
      }


      Controller.addPaymentIntegration({
        funnel: funnel,
        payment:payment
      })
      .then((result) => {
        console.log(result)
        success('Success','Payment Meathod Added Successfully!')
          .then(() => {
            window.location.reload();
          })
          .catch(err => {
            console.log(err);
          })
      })
      .catch(err => {
        console.log(err)
        oops('Error in Adding Payment Methoad')
      })
    })

 
    var pageId = "<%= funnel.steps[0]._id %>";

    Controller.getPageIntegartions({
        pageId: pageId,
        type: 'Payment'
    })
    .then((data) => {

      if (data.integration) {
        addPayment.hide();
        removePayment.show();
        removePayment.attr('payment', data.details._id);
        selected.val(data.details._id)
      }
    })
    .catch(err => {
        console.log(err)
        oops('Something Went Wrong in Get Page Integrations')
    })



    removePayment.on('click', function(e) {
      e.preventDefault()

      let funnelId = ($(this).attr('funnel'));
      let paymentId = ($(this).attr('payment'));

      confirmation('Remove Payment Integration?', 'are you want to remove this payment Integartion?', 'Ok')
        .then((result) => {
            if (result.value) {
              Controller.removePaymentIntegration({
                funnel: funnelId,
                payment:paymentId
              })
              .then((result) => {
                console.log(result)
                window.location.reload()
              })
              .catch(err => {
                console.log(err)
              })
            }
        })
        .catch(err => {
          console.log(err)
        })
     

    })


  })
</script>
